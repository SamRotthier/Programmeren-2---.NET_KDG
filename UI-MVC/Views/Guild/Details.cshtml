@model MedievalMMO.BL.Domain.Guild

@{
    ViewBag.Title = "Guild Details";
    Layout = "_Layout";
}

<h2>Guild Details</h2>

<p><strong>Guild Id: </strong><span>@Model.GuildId</span></p>
<p><strong>Guild Name: </strong><span>@Model.GuildName</span></p>
<p><strong>Guild Made On: </strong><span>@Model.GuildMadeOn.ToString("yyyy/MM/dd")</span></p>
<p><strong>Guild Level: </strong><span>@(Model.GuildLevel?.ToString() ?? "No Known Level")</span></p>
<p><strong>Guild Leader: </strong><span>@(Model.GuildMadeBy ?? "No Known Owner")</span></p>

<strong><a asp-controller="Player" asp-action="Index">Back to Player Selection</a></strong>